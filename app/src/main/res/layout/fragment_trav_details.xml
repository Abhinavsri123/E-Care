<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ecare.newu.e_care.public_portal.trav_details">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView28"
        android:textSize="18sp"
        android:layout_alignTop="@+id/textView29"
        android:layout_alignStart="@+id/textView27"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="Time"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView29"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/textView25"
        android:layout_alignStart="@+id/textView25" />

    <TextView
        android:text="TextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView27"
        android:textSize="18sp"
        android:layout_above="@+id/textView29"
        android:layout_toEndOf="@+id/textView30" />

    <TextView
        android:text="Date:"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView25"
        android:layout_marginTop="27dp"
        android:layout_marginStart="13dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:text=""
        android:layout_width="wrap_content"
        android:layout_alignTop="@+id/textView26"
        android:layout_alignStart="@+id/spinner"
        android:id="@+id/textView31"
        android:textSize="14sp"
        android:layout_alignParentEnd="true"
        android:hint="Vehical Number"
        android:layout_height="35dp" />

    <TextView
        android:text="Vehical No."
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:id="@+id/textView26"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/spinner"
        android:layout_alignStart="@+id/textView30"
        android:layout_height="40dp" />

    <Button
        android:text="Start Tracking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_marginTop="70dp"
        android:layout_below="@+id/textView31"
        android:layout_centerHorizontal="true" />

    <Spinner
        android:layout_width="match_parent"
        android:id="@+id/spinner"
        android:layout_alignBottom="@+id/textView30"
        android:layout_toEndOf="@+id/textView30"
        android:layout_height="30dp" />

    <TextView
        android:text="Vehical Type"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:id="@+id/textView30"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/textView28"
        android:layout_alignStart="@+id/textView29"
        android:layout_height="30dp" />

</RelativeLayout>
